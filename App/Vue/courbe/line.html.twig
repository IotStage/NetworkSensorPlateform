<script type="text/javascript">

    Highcharts.chart('graph{{ courbe.id }}', {
        chart: {
            type: '{{ courbe.type }}',
            zoomType: 'x'
        },
        title: {
            text: '{{ courbe.title }}'
        },
        subtitle: {
            text: document.ontouchstart === undefined ?
                'cliquez et glisser pour zoomer' : 'cliquez pour dezoomer',
        },
        xAxis: {
            type: 'datetime',
            dateTimeLabelFormats: {
                hour: '%a %d %b'    //ex- 01 Jan 2016

            }
            //categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']

        },
        yAxis: {
            title: {
                text: '{{ courbe.nom }}'
            }
        },
    plotOptions: {
        area: {
            fillColor: {
                linearGradient: {
                    x1: 0,
                    y1: 0,
                    x2: 0,
                    y2: 1
                },
                stops: [
                    [0, Highcharts.getOptions().colors[0]],
                    [1, Highcharts.Color(Highcharts.getOptions().colors[0]).setOpacity(0).get('rgba')]
                ]
            },
            marker: {
                radius: 2
            },
            lineWidth: 1,
            states: {
                hover: {
                    lineWidth: 1
                }
            },
            threshold: null
        }
    },
        series: [{
            name: '{{ courbe.nom }}',
            type: "area",
            data: [
                {% for a in courbe.datas%}
                    [{{date( a[0] ).timestamp}}, {{ a[1] }}],
                {% endfor %}
            ]
        }]
    });

</script>