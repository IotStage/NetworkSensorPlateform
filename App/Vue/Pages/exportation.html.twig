{% extends 'template.html.twig' %}
{% block body%}
<div class="row">{% include 'nav.html.twig' %}<br><br><br></div>
{% block content %}
<div class="row">
    {% include "menu.graph.html.twig" %}
    <div class="col-md-4">
        <div class="panel panel-info panneau">
            <div class="panel-heading">
                <div class="valeur">
                    <h4 style="text-align: center">Variataion du PH</h4>

                </div>
            </div>
            <div class="panel-body">

                <div id="graph"></div>
            </div>
        </div>
    </div>
</div>


{% endblock %}

{% block chart %}
    <script type="text/javascript">
        Highcharts.chart('graph', {{ graph | raw }});
    </script>

{% endblock %}
{% block highchart %}
    <script type="text/javascript" src="public/dist/js/highcharts.js"></script>
    <script type="text/javascript" src="public/dist/js/modules/exporting.js"></script>
{% endblock %}
{% endblock %}



