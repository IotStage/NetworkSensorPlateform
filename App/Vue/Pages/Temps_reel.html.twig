        <ul class="nav nav-tabs" style="margin: 25px;" data-no-turbolink="">
            <li role="presentation" class="active">
                <a href="{{ path_for('graph') }}/tab/1" >Temps reel</a>
            </li>
            <li role="presentation">
                <a href="{{ path_for('graph') }}/tab/2" >Visualisation</a>
            </li>
            <li role="presentation">
                <a href="{{ path_for('graph') }}/tab/3">Parametres Maladies</a>
            </li>
            <li role="presentation">
                <a href="{{ path_for('graph') }}/tab/4">Exportation des donn√©es</a>
            </li>
        </ul>
    </div>
    {% for key, cou in  graph %}
        <div class="col-md-6">
            {% include 'graph.panel.twig' with {'id': key } %}

        </div>
    {% endfor %}


</div>


{% endblock %}

{% block chart %}
    {% for key, courbe in  graph %}
        {% include 'courbe/line.html.twig' with {'courbe': courbe } %}
